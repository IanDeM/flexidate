/*------------ALGEMENE OPMAAK-------------*/
body {
    font-family: Verdana, Geneva, Tahoma, sans-serif;
    font-size: 0.8em;
}

#containerSite {
    width: auto;
    color: rgb(78, 96, 120);
}

/*------------WELKOMSTPAGINA--------------*/
#column1 {
    display: inline-block;
    float: left;
    width: 60%;
    background-image: linear-gradient(to left, rgba(255, 0, 0, 0), rgba(78, 96, 120, 1));
    height: inherit;
    text-align: center;
}

#column2 {
    display: inline-block;
    float: left;
    width: 40%;
    background-color: white;
    height: inherit;
}

@media screen and (max-width: 600px) {
    #column1 {
        display: none;
    }

    #column2 {
        width: 100%;
        background-color: rgba(78, 96, 120, 0.2);
    }
}

#manwoman {
    padding-top: 7%;
}

#containerLogin {
    /*border: 8px outset rgb(78, 96, 120);*/
    border-style: solid;
    border-width: 8px;
    border-top-color: rgb(78, 96, 120);
    border-left-color: rgb(78, 96, 120);
    border-right-color: rgba(78, 96, 120, 0.5);
    border-bottom-color: rgba(78, 96, 120, 0.5);
    border-radius: 20px;
    height: 80%;
    margin: 10% 20%;
    position: relative;
    text-align: center;
    overflow: hidden;
}

#loginLabel {
    display: inline-block;
    margin-top: 20%;
    margin-bottom: 10%;
    font-size: 2em;
    font-weight: bold;
}

#inschrijvenLabel {
    display: inline-block;
    margin-top: 20%;
    margin-bottom: 10%;
    font-size: 2em;
    font-weight: bold;
}

#loginOptie {
    display: none;
}

#login_email {
    display: inline-block;
    margin-bottom: 10%;
}

#email {
    display: inline-block;
    margin-bottom: 10%;
}

/*-----------HEADER-----------------*/
#header {
    height: 150px;
    /*border-bottom: white solid 1px;*/
}

header nav {
    float: right;
}

@media screen and (max-width: 600px) {
    header nav {
        float: left;
    }
}

#banner {
    float: left;
    width: 60%;
    background: url("../img/flexidatingBanner.jpg") no-repeat;
    background-size: cover;
    height: inherit;
}

@media screen and (max-width: 600px) {
    #banner {
        width: 100%;
    }
}

#menuTopRight {
    float: left;
    width: 40%;
    background-color: rgb(78, 96, 120);
    height: inherit;
    position: relative;
}

@media screen and (max-width: 600px) {
    #menuTopRight {
        clear: both;
        width: 100%;
        background-color: rgb(78, 96, 120);
        height: auto;
        position: relative;
    }
}

/*----------------MIJN PROFIEL-------------*/
#kolom2a input, #kolom2a select {
    width: 150px;
    border-radius: 5px;
    padding: 5px 0 5px 10px;
}

#kolom2a select {
    width: 150px;
}

#kolom2a input, #kolom2a select {
    /*padding-right: -100px;*/
    float: right;
}

#kolom2 p {
    margin-top: 20px;
    margin-bottom: 20px;
}

@media screen and (max-width: 600px) {
    #kolom2a input, #kolom2a select {
        padding: unset;
    }

    #kolom2a select {
        width: 150px;
    }
}

#nickname {
    display: inline-block;
    margin-bottom: 10%;
}

/*------------OPMAAK KNOPPEN EN LIJSTEN-------------*/
button {
    background-color: white;
    color: rgb(78, 96, 120);
    border-style: none;
    width: auto;
    padding: 10px;
    border-radius: 20px;
    float: right;
    margin-top: 80px;
    display: inline;
}

@media screen and (max-width: 930px) and (min-width: 601px) {
    button {
        margin-top: unset;
    }
}

@media screen and (max-width: 600px) {
    button {
        float: unset;
        margin-top: unset;
    }
}

button {
    font-size: 12px;
    text-transform: uppercase;
}

button:hover {
    background-color: rgba(78, 96, 120, 0.2);
    color: white;
}

header a {
    color: #ffffff;
    text-decoration: none;
}

header li, li {
    float: left;
    display: inline;
    padding: 0 5px 0 5px;
    text-decoration: none;
}

#eersteZoek li {
    padding: 0 10px 0 5px;
}

@media screen and (max-width: 930px) and (min-width: 601px) {
    header li, li {
        float: unset;
        display: list-item;
        padding: 10px 10px 10px 0;
    }

    #contact li {
        display: inline;
    }

    #eersteZoek li {
        display: inline-block;
    }

    #nicknameClass li {
        display: inline-block;
    }
}

@media screen and (max-width: 600px) {
    header li, li {
        padding: 0 15px 15px 0;
        display: list-item;
        float: left;
    }
}

.btn_14 {
    background-color: rgb(78, 96, 120);
    color: white;
}

.btn_14:hover {
    color: black;
}

/*-------------PROFIELEN OVERZICHT--------------*/
img {
    margin-right: 1em;
    float: left;
}

.thumb {
    border-style: solid;
    border-width: 2px;
    border-color: blue;
    background: white;
    width: 110px;
    height: 130px;
    padding: 0.625em;
    margin: 0.625em;
    float: left;
}

.thumb:hover {
    background: #99ffff;
    color: #0000cc;
}

.caption {
    clear: left;
}

/*--------------PROFIELEN ALGEMENE OPMAAK---------------*/
#midden {
    clear: both;
    height: 75vh;
    border-bottom: 2px solid white;
    width: 100%
}

.footerOverflow #midden {
    height: 95vh;
}

@media screen and (max-width: 600px) {
    .footerOverflow #midden {
        height: 110vh;
        background-color: rgba(78, 96, 120, 0.2);
    }
}

#kolom1 {
    float: left;
    width: 20%;
    background-image: linear-gradient(to left, rgba(78, 96, 120, 0.2), rgba(78, 96, 120, 1));
    height: inherit;
    color: white;
}

#kolom2 {
    float: left;
    width: 60%;
    background-color: rgba(78, 96, 120, 0.2);
    height: inherit;
}

#kolom2a {
    float: left;
    padding-right: 10%;
    padding-left: 10%;
    width: 50%;
    padding-top: 5%;
}

#kolom2b {
    float: left;
    width: 30%;
    padding-top: 10%;
}

@media screen and (max-width: 600px) {
    #kolom2a {
        width: 80%;
    }

    #kolom2b {
        clear: both;
        width: 30%;
        margin-left: 35%;
        margin-right: 35%;
        padding-top: unset;
    }
}

#kolom3 {
    float: left;
    width: 20%;
    background-image: linear-gradient(to right, rgba(78, 96, 120, 0.2), rgba(78, 96, 120, 1));
    height: inherit;
    text-align: center;
}

#containerOverzicht {
    text-align: center;
    margin-top: 50px;
    height: 85%;
    overflow-Y: scroll;
}

@media screen and (max-width: 600px) {
    #kolom2, #kolom3 {
        float: unset;
        width: 100%;
        height: auto;
        background-color: rgba(78, 96, 120, 0.2);
    }

    #kolom1 {
        width: 100%;
        height: auto;
        background-image: linear-gradient(to top, rgba(78, 96, 120, 0.2), rgba(78, 96, 120, 1));
    }

    #kolom2 {
        clear: both;
    }
}

@media screen and (max-width: 1100px) and (min-width: 601px) {
    .bodyProfielen #kolom1 {
        width: 30%;
        padding-right: 10%;
    }

    .bodyProfielen #kolom2 {
        width: 60%;
    }

    .bodyProfielen #kolom3 {
        width: 100%;
        height: auto;
    }

}

/*-------------------PROGRESSBAR---------------*/
#progressBar {
    background-color: white;
    height: 30px;
    width: 100px;
    margin: auto;
    border: white solid 1px;
}

#progressBarInside {
    height: 30px;
    background-color: rgba(78, 96, 120, 0.8);
    width: 1px;
    margin-left: auto;
    margin-right: auto;
    vertical-align: middle;
}

#progressBarInside {
    background-color: greenyellow;
    float: left;
}

#progressBarInside.progressNotFull {
    background-color: rgb(255, 147, 47);
    float: left;
}

#barText {
    height: 40px;
    /*display: table-cell; */
    vertical-align: middle;
    padding-top: 10px;
    color: black;
}

/*---------------SEARCHBAR---------------*/
#kolom1 #openSearch, #kolom1 #closeSearch {
    font-size: 16px;
    margin-top: 10px;
}

#search input, #search select {
    float: left;
}

#eersteZoek {
    margin-top: 40px;
}

@media screen and (max-width: 600px) {
    #eersteZoek {
        margin-top: 20px;
        width: 250px;
        margin-left: auto;
        margin-right: auto;
    }
}

