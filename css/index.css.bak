@media screen and (max-width: 1100px) and (min-width: 601px) {
    #eersteZoek {
        margin-top: 20px;
        width: 200px;
        margin-left: 10px;
        list-style-type: none;
    }
    #boodschapZoekMogelijkheden {}
}
