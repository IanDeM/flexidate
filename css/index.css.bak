#eersteZoek button {
    width: 80%;
    font-size: x-small;
    margin: 10px 0 10px 0;
    float: left;
    text-transform: none;
}

#eersteZoek li {
    margin: 5px 0 5px 0;
    position: relative;
}

@media screen and (max-width: 600px) {
    #eersteZoek li {
        margin: 5px 0 5px 0;
        position: relative;
        display: unset;
    }
}

#voortgangProfiel {
    color: white;
    text-align: center;
}

/*----------------CONTACTFORMULIER VIEWPROFILE------------*/
#contact {
    margin-left: 10%;
    padding-top: 20px;
    width: 100%;
    display: block;
    clear: both;
}

#contact button {
    margin-left: -50px;
    margin-top: 0px;
}

#contact fieldset {
    position: relative;
    width: 80%;
    font-size: 1.2em;
}

textarea {
    width: 100%;
    height: 150px;
    padding: 12px 20px;
    box-sizing: border-box;
    border: 2px solid #ccc;
    border-radius: 4px;
    background-color: rgba(78, 96, 120, 0.2);
    resize: none;
    color: black;
    font-family: Verdana, Geneva, Tahoma, sans-serif;
    font-size: 0.8em;
}

/*-------------------MAIL WEBMASTER--------------*/
#containerMail {
    /*border: 8px outset rgb(78, 96, 120);*/
    border-style: solid;
    border-width: 8px;
    border-top-color: rgb(78, 96, 120);
    border-left-color: rgb(78, 96, 120);
    border-right-color: rgba(78, 96, 120, 0.5);
    border-bottom-color: rgba(78, 96, 120, 0.5);
    border-radius: 20px;
    height: 50%;
    margin: 10% 20%;
    position: relative;
    text-align: center;
}

/*-------------FOOTER-----------------*/
footer {
    clear: both;
    left: 0;
    bottom: 0;
    width: 100%;
    background-color: white;

    text-align: center;
    padding-top: 20px;
    padding-bottom: 20px;
}

#footer button {
    margin: auto auto;
    background-color: rgb(78, 96, 120);
    color: white;
}

#footer button:hover {

    background-color: inherit;
    color: black;
    border: 1px solid black;
}

